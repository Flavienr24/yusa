      <%= image_tag "illustrations/home-illustr.svg", class: 'width-40-xl mh-100', alt: 'Yusa home illustration' %>
    </div>
    <%= link_to root_path(anchor:'start'), class: 'center-item' do %>
    <%= image_tag 'arrow-d-black.svg', class: 'mb-5' %>
    <% end %>
  </div>
  <div class="content-container mb-5">
    <div id='start' class="rubrique-title text-center top-space">Vous souhaitez mettre le digital au centre de votre proposition de valeur ?</div>
